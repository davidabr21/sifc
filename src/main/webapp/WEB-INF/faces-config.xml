<?xml version="1.0" encoding="UTF-8"?>
<faces-config version="2.0"
    xmlns="http://java.sun.com/xml/ns/javaee" 
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd">
     <application>
     	   <locale-config>
     	        <default-locale>es</default-locale>
     	        <supported-locale>en</supported-locale>
     	   </locale-config>
	   <resource-bundle>
		<base-name>com.duopharma.lang</base-name>
		<var>translate</var>
	   </resource-bundle>
     </application>
    <navigation-rule>
        <from-view-id>*</from-view-id>
        <navigation-case>
            <from-outcome>inicio</from-outcome>
            <to-view-id>index.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>contactenos</from-outcome>
            <to-view-id>/landing-page/contactenos.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>login</from-outcome>
            <to-view-id>/landing-page/login.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>quienesSomos</from-outcome>
            <to-view-id>/landing-page/quienes_somos.xhtml</to-view-id>
        </navigation-case>
         <navigation-case>
            <from-outcome>carrito</from-outcome>
            <to-view-id>/pages/Carritol.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>profile</from-outcome>
            <to-view-id>/pages/Profile.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>registrar</from-outcome>
            <to-view-id>/landing-page/registrar.xhtml</to-view-id>
        </navigation-case>
		<navigation-case>
            <from-outcome>tipoOperacion</from-outcome>
            <to-view-id>/pages/TipoOperacion.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
</faces-config>